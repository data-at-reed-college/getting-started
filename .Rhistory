# Leftovers #########################################################
leftovers_graph <- leftover %>%
arrange(Div, Major, Yr, Leftover)
leftovers_graph$Major <- as.factor(leftovers_graph$Major)
leftovers_graph$Div <- as.factor(leftovers_graph$Div)
leftovers_graph$Yr <- as.factor(leftovers_graph$Yr)
p1 <- ggplot(leftovers_graph, aes(x = Major, y = Leftover, fill = Div)) +
geom_bar(stat = "identity", position = "dodge") +
scale_fill_manual(values = c("seagreen4", "deepskyblue3", "goldenrod1", "salmon", "magenta4", "darkblue"),
name = "Division") +
scale_x_discrete(limits = unique(leftovers_graph$Major)) +
ylab("Unmatched") +
theme_bw() +
theme(axis.text.x = element_text(angle = 90, vjust = 0.5, hjust=1)) +
theme(axis.title = element_text(face = "bold", size = 14),
legend.title = element_text(face = "bold", size = 12))
p2 <- ggplot(leftovers_graph, aes(x = Major, y = Leftover, fill = Yr)) +
geom_bar(stat = "identity", position = "dodge") +
scale_fill_manual(values = c("seagreen4", "deepskyblue3", "goldenrod1", "salmon"),
name = "Year") +
scale_x_discrete(limits = unique(leftovers_graph$Major)) +
ylab("Unmatched") +
theme_bw() +
theme(axis.text.x = element_text(angle = 90, vjust = 0.5, hjust=1)) +
theme(axis.title = element_text(face = "bold", size = 14),
legend.title = element_text(face = "bold", size = 12))
unmatch <- grid.arrange(p1, p2, ncol = 1)
unmatch
